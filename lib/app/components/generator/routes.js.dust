define(function () {
    'use strict';

    return function () {
        {#routes}
            this.{?method}{method.toLowerCase}{:else}match{/method}('{uri}'){?conds}
                .where({conds|l|s}){/conds}
                .to('{terminal}'){?opts}
                .options({opts|l|s}){/opts}{?name}
                .as('{name}'){/name};
        {/routes}
    };
});
